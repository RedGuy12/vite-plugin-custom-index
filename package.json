{
  "publishConfig": {
    "access": "public"
  },
  "name": "vite-plugin-custom-index",
  "version": "0.1.1",
  "description": "Define a custom file for Vite to use as your index.html file",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdf221/vite-plugin-custom-index"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src/index.ts --dts --format cjs,esm",
    "dev": "npm run build -- --watch"
  },
  "author": "Jared (jdf2)",
  "license": "MIT",
  "devDependencies": {
    "@jdf221/eslint-config-typescript": "^0.1.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^15.3.0",
    "eslint": "^7.26.0",
    "prettier": "^2.3.0",
    "tsup": "^4.11.0",
    "vite": "^2.3.3"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "pretty-format": "^26.6.2"
  }
}
